{
    "name": "rishadblack/laraform",
    "description": "Laraform",
    "license": "MIT",
    "authors": [
        {
            "name": "S M Rishad",
            "email": "rishad.black@gmail.com",
            "homepage": "https://smrishad.com"
        }
    ],
    "homepage": "https://github.com/rishadblack/laraform",
    "keywords": [
        "Laravel",
        "Laraform"
    ],
    "version": "0.0.1",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rishadblack\\Laraform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rishadblack\\Laraform\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rishadblack\\Laraform\\LaraformServiceProvider"
            ],
            "aliases": {
                "Laraform": "Rishadblack\\Laraform\\Facades\\Laraform"
            }
        }
    }
}